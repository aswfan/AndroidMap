     <item>闺蜜</item>
    </string-array>
    <string-array name="fee">
     <item>请选择</item>
     <item>0~999</item>  
     <item>1000~2999</item>
     <item>3000~4999</item>
     <item>5000~9999</item>
     <item>>10000</item>
