<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="headerAttr">
        <attr name="myTitle" format="string"/>
        <attr name="myTitleSize" format="dimension"/>
        <attr name="myTitleColor" format="color"/>
        <attr name="leftBackground" format="color|reference"/>
        <attr name="leftSrc" format="color|reference"/>
        <attr name="rightBackground" format="color|reference"/>
        <attr name="rightTextColor" format="color"/>
        <attr name="rightText" format="string"/>
    </declare-styleable>
    
    <string-array name="date">
     <item>请选择</item>
     <item>1-3月</item>  
     <item>4-6月</item>
     <item>7-9月</item>
     <item>10-12月</item>
    </string-array>
    <string-array name="days">
